{
	"name": "claude-dev",
	"displayName": "Cool Cline",
	"description": "Autonomous coding agent right in your IDE, capable of creating/editing files, running commands, using the browser, and more with your permission every step of the way.",
	"version": "3.0.9",
	"icon": "assets/icons/icon.png",
	"galleryBanner": {
		"color": "#617A91",
		"theme": "dark"
	},
	"engines": {
		"vscode": "^1.84.0"
	},
	"author": {
		"name": "Cline Bot Inc."
	},
	"license": "Apache-2.0",
	"publisher": "saoudrizwan",
	"repository": {
		"type": "git",
		"url": "https://github.com/chatterzhao/cool-cline"
	},
	"homepage": "https://cline.bot",
	"categories": [
		"AI",
		"Chat",
		"Programming Languages",
		"Education",
		"Snippets",
		"Testing"
	],
	"keywords": [
		"cline",
		"claude",
		"dev",
		"mcp",
		"openrouter",
		"coding",
		"agent",
		"autonomous",
		"chatgpt",
		"sonnet",
		"ai",
		"llama"
	],
	"activationEvents": [],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "claude-dev-ActivityBar",
					"title": "Cool Cline",
					"icon": "$(robot)"
				}
			]
		},
		"views": {
			"claude-dev-ActivityBar": [
				{
					"type": "webview",
					"id": "claude-dev.SidebarProvider",
					"name": ""
				}
			]
		},
		"commands": [
			{
				"command": "cline.plusButtonClicked",
				"title": "New Task",
				"icon": "$(add)"
			},
			{
				"command": "cline.mcpButtonClicked",
				"title": "MCP Servers",
				"icon": "$(server)"
			},
			{
				"command": "cline.historyButtonClicked",
				"title": "History",
				"icon": "$(history)"
			},
			{
				"command": "cline.popoutButtonClicked",
				"title": "Open in Editor",
				"icon": "$(link-external)"
			},
			{
				"command": "cline.settingsButtonClicked",
				"title": "Settings",
				"icon": "$(settings-gear)"
			},
			{
				"command": "cline.openInNewTab",
				"title": "Open In New Tab",
				"category": "Cline"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "cline.plusButtonClicked",
					"group": "navigation@1",
					"when": "view == claude-dev.SidebarProvider"
				},
				{
					"command": "cline.mcpButtonClicked",
					"group": "navigation@2",
					"when": "view == claude-dev.SidebarProvider"
				},
				{
					"command": "cline.historyButtonClicked",
					"group": "navigation@3",
					"when": "view == claude-dev.SidebarProvider"
				},
				{
					"command": "cline.popoutButtonClicked",
					"group": "navigation@4",
					"when": "view == claude-dev.SidebarProvider"
				},
				{
					"command": "cline.settingsButtonClicked",
					"group": "navigation@5",
					"when": "view == claude-dev.SidebarProvider"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "bun run package",
		"compile": "bun x tsc --noEmit && bun x eslint src --ext ts && bun run esbuild.js",
		"watch": "concurrently \"bun run watch:esbuild\" \"bun run watch:tsc\"",
		"watch:esbuild": "bun run esbuild.js --watch",
		"watch:tsc": "bun x tsc --noEmit --watch --project tsconfig.json",
		"package": "bun run build:webview && bun run check-types && bun run lint && bun run esbuild.js --production",
		"compile-tests": "bun x tsc -p ./tsconfig.test.json --outDir dist",
		"watch-tests": "bun x tsc -p . -w --outDir dist",
		"pretest": "bun x tsc -p ./tsconfig.test.json --outDir dist",
		"check-types": "bun x tsc --noEmit",
		"lint": "bun x eslint src --ext ts",
		"format": "bun x prettier . --check",
		"format:fix": "bun x prettier . --write",
		"test": "bun x vscode-test",
		"install:all": "bun install && cd webview-ui && bun install",
		"start:webview": "cd webview-ui && bun run start",
		"build:webview": "cd webview-ui && bun run build",
		"test:webview": "cd webview-ui && bun run test",
		"publish:marketplace": "bun x vsce publish && bun x ovsx publish"
	},
	"devDependencies": {
		"@types/diff": "^5.2.3",
		"@types/mocha": "^10.0.10",
		"@types/node": "^20.17.10",
		"@types/should": "^11.2.0",
		"@types/vscode": "^1.96.0",
		"@typescript-eslint/eslint-plugin": "^8.19.0",
		"@typescript-eslint/parser": "^8.19.0",
		"@vscode/test-cli": "^0.0.9",
		"@vscode/test-electron": "^2.4.1",
		"esbuild": "^0.21.5",
		"eslint": "^8.57.1",
		"prettier": "^3.4.2",
		"should": "^13.2.3",
		"typescript": "^5.7.2",
		"concurrently": "^8.2.2"
	},
	"dependencies": {
		"@anthropic-ai/bedrock-sdk": "^0.10.4",
		"@anthropic-ai/sdk": "^0.26.1",
		"@anthropic-ai/vertex-sdk": "^0.4.3",
		"@google/generative-ai": "^0.18.0",
		"@modelcontextprotocol/sdk": "^1.0.4",
		"@types/clone-deep": "^4.0.4",
		"@types/pdf-parse": "^1.1.4",
		"@types/turndown": "^5.0.5",
		"@vscode/codicons": "^0.0.36",
		"axios": "^1.7.9",
		"cheerio": "^1.0.0",
		"chokidar": "^4.0.3",
		"clone-deep": "^4.0.1",
		"default-shell": "^2.2.0",
		"delay": "^6.0.0",
		"diff": "^5.2.0",
		"execa": "^9.5.2",
		"fast-deep-equal": "^3.1.3",
		"globby": "^14.0.2",
		"isbinaryfile": "^5.0.4",
		"mammoth": "^1.9.0",
		"monaco-vscode-textmate-theme-converter": "^0.1.7",
		"openai": "^4.77.0",
		"os-name": "^6.0.0",
		"p-wait-for": "^5.0.2",
		"pdf-parse": "^1.1.1",
		"puppeteer-chromium-resolver": "^23.0.0",
		"puppeteer-core": "^23.11.1",
		"serialize-error": "^11.0.3",
		"strip-ansi": "^7.1.0",
		"tree-sitter-wasms": "^0.1.12",
		"turndown": "^7.2.0",
		"web-tree-sitter": "^0.22.6",
		"zod": "^3.24.1"
	}
}
